{
  "name": "aws-credentials-vscode-extension",
  "displayName": "Aws CLI Credentials",
  "description": "Setup your local Aws CLI Credentials, Monitor Expiration Time and Auto Refresh Token",
  "version": "1.0.0",
  "publisher": "NecatiARSLAN",
  "repository": {
    "type": "git",
    "url": "https://github.com/necatiarslan/aws-access-vscode-extension"
  },
  "icon": "media/aws-credentials-logo.png",
  "engines": {
    "vscode": "^1.71.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "aws-credentials-vscode-extension.RefreshCredentials",
        "title": "Aws Credentials:Refresh Credentials"
      },
      {
        "command": "aws-credentials-vscode-extension.SetAwsLoginCommand",
        "title": "Aws Credentials:Set Login Command"
      },
      {
        "command": "aws-credentials-vscode-extension.ListAwsProfiles",
        "title": "Aws Credentials:List Profiles"
      },
      {
        "command": "aws-credentials-vscode-extension.RunLoginCommand",
        "title": "Aws Credentials:Run Login Command"
      },
      {
        "command": "aws-credentials-vscode-extension.PauseAutoLogin",
        "title": "Aws Credentials:Pause/UnPause Auto Login"
      },
      {
        "command": "aws-credentials-vscode-extension.SetActiveProfile",
        "title": "Aws Credentials:Set Active Profile"
      },
      {
        "command": "aws-credentials-vscode-extension.ShowActiveCredentials",
        "title": "Aws Credentials:Show Active Credentials"
      },
      {
        "command": "aws-credentials-vscode-extension.ShowDefaultCredentials",
        "title": "Aws Credentials:Show Default Credentials"
      },
      {
        "command": "aws-credentials-vscode-extension.OpenCredentialsFile",
        "title": "Aws Credentials:Open Credentials File"
      },
      {
        "command": "aws-credentials-vscode-extension.OpenConfigFile",
        "title": "Aws Credentials:Open Config File"
      },
      {
        "command": "aws-credentials-vscode-extension.TestAwsConnectivity",
        "title": "Aws Credentials:Test Aws Connectivity With Active User"
      },
      {
        "command": "aws-credentials-vscode-extension.CopyCredentialsToDefaultProfile",
        "title": "Aws Credentials:Copy Credentials To Default Profile"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.71.0",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "@vscode/test-electron": "^2.1.5",
    "eslint": "^8.18.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@aws-sdk/credential-provider-node": "^3.181.0",
    "aws-sdk": "^2.1276.0"
  }
}
